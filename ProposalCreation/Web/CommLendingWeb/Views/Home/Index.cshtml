@model CommLendingWeb.Models.HomeModel;
@*Globalization*@
<script type="text/javascript">
	(function ()
	{
		var localizationStrings = {};
		@foreach (var item in Model.Resources)
		{
			@:localizationStrings["@item.Key"] = "@Html.Raw(item.Value)";
		}

		window.stringResources = localizationStrings;
	})();
</script>

<script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/react/15.4.0/react.js"></script>
